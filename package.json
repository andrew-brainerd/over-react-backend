{
  "name": "over-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "overwatch-api": "^0.4.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.4",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  },
  "scripts": {
    "start": "yarn dev",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server": "yarn && nodemon server.js",
    "client": "cd client && yarn start",
    "fresh": "yarn && cd client && yarn",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  }
}
